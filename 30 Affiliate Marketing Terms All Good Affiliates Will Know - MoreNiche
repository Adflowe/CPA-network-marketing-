.elementor--v-position-bottom .swiper-slide-inner { -webkit-box-align: end; align-items: flex-end; }

.elementor--v-position-middle .swiper-slide-inner { -webkit-box-align: center; align-items: center; }

.elementor--h-position-left .swiper-slide-inner { -webkit-box-pack: start; justify-content: flex-start; }

.elementor--h-position-right .swiper-slide-inner { -webkit-box-pack: end; justify-content: flex-end; }

.elementor--h-position-center .swiper-slide-inner { -webkit-box-pack: center; justify-content: center; }

